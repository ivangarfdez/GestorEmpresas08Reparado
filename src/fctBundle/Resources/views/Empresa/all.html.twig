<table>
    <tr>
        <td>#</td>
        <td>Nombre</td>
        <td>Dirección</td>
        <td>Código Postal</td>
        <td>Teléfono 1</td>
        <td>Teléfono 2</td>
        <td>Fecha de Creación</td>
    </tr>
    {% for empresa in empresa %}
        <tr>
            <td>{{ empresa.id }}</td>
            <td>{{ empresa.nombre }}</td>
            <td>{{ empresa.direccion }}</td>
            <td>{{ empresa.cp }}</td>
            <td>{{ empresa.telefono1 }}</td>
            <td>{{ empresa.telefono2 }}</td>
            <td>{{ empresa.fecha.date }}</td>
        </tr>
    {% endfor %}
</table>
